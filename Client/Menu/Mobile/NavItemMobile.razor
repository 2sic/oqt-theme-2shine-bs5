@inherits MenuBase;

@if(PageNavigator != null)
{
    <li class="@liCLasses()">
        <a class="@aClasses()" target="" data-bs-toggle="@aDataBsToggle()" href="@aHref()">
            @PageNavigator.CurrentPage.Name
        </a>

        @if(PageNavigator.HasChildren)
        {
            <span class="dropdown-toggle nav-item-sub-opener" ></span>
            <NavItemsMobile PageNavigator="PageNavigator"></NavItemsMobile>
        }
    </li>
}