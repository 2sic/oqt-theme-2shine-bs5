@inherits NavEntryBase
@using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Main;
@using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Mobile;
@using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Submenu;


@switch (Variation)
{
    case "Submenu":
        <NavItemsSubmenu PageNavigator="Start"></NavItemsSubmenu>
        break;

    case "Mobile":
        <NavItemsMobile PageNavigator="Start"></NavItemsMobile>
        break;
    default: //Main
    {
        <NavItems PageNavigator="Start"></NavItems>
        break;
    }
}

@code{
    [Parameter]
    public string Variation { get; set; }
}
