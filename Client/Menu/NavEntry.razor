@inherits NavEntryBase
@    using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Main;
@    using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Mobile;
@    using ToSic.Oqt.Themes.ToShineBs5.Client.Menu.Sidebar;
    using ToSic.Oqt.Themes.ToShineBs5.Client.Nav;


@switch (Variation)
{
    case "Sidebar":
        <NavItemsSidebar PageNavigator="Start"></NavItemsSidebar>
        break;
    case "Mobile":
        <NavItemsMobile PageNavigator="Start"></NavItemsMobile>
        break;
    default: //Main
        <NavItems PageNavigator="Start"></NavItems>
        break;
}