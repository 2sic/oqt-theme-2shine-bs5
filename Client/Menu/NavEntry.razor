@inherits NavEntryBase
@using ToSic.Oqt.Themes.ToShineBs5.Client.Nav;
@using ToSic.Oqt.Themes.ToShineBs5.Client.Menu;

@switch (Variation)
{
    case "Sidebar":
        <NavItemsSidebar PageNavigator="Start"></NavItemsSidebar>
        break;
    case "Mobile":
        <NavItemsMobile PageNavigator="Start"></NavItemsMobile>
        break;
    case "Main": 
        <NavItems PageNavigator="Start"></NavItems>
        break;
    default: //Main
        <NavItems PageNavigator="Start"></NavItems>
        break;
}