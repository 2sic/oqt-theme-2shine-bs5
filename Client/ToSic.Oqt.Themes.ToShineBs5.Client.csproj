<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RazorLangVersion>3.0</RazorLangVersion>
    <Version>1.0.0</Version>
    <Authors>tosic</Authors>
    <Company>tosic</Company>
    <Description>[Description]</Description>
    <Product>ToSic.Oqt.Themes.ToShineBs5</Product>
    <Copyright>tosic</Copyright>
    <AssemblyName>ToSic.Oqt.Themes.ToShineBs5.Client.Oqtane</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CenteredSubmenu\Containers\**" />
    <Compile Remove="Centered\Containers\**" />
    <Compile Remove="FloatWideHeader\Containers\**" />
    <Compile Remove="Fullscreen\Containers\**" />
    <Content Remove="CenteredSubmenu\Containers\**" />
    <Content Remove="Centered\Containers\**" />
    <Content Remove="FloatWideHeader\Containers\**" />
    <Content Remove="Fullscreen\Containers\**" />
    <EmbeddedResource Remove="CenteredSubmenu\Containers\**" />
    <EmbeddedResource Remove="Centered\Containers\**" />
    <EmbeddedResource Remove="FloatWideHeader\Containers\**" />
    <EmbeddedResource Remove="Fullscreen\Containers\**" />
    <None Remove="CenteredSubmenu\Containers\**" />
    <None Remove="Centered\Containers\**" />
    <None Remove="FloatWideHeader\Containers\**" />
    <None Remove="Fullscreen\Containers\**" />
    <TypeScriptCompile Remove="CenteredSubmenu\Containers\**" />
    <TypeScriptCompile Remove="Centered\Containers\**" />
    <TypeScriptCompile Remove="FloatWideHeader\Containers\**" />
    <TypeScriptCompile Remove="Fullscreen\Containers\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="src\scss\components\_page-customizations.scss" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="src\scss\_page-customizations.scss" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0" PrivateAssets="all" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="6.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Localization" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Localization" Version="6.0.0" />
    <PackageReference Include="System.Net.Http.Json" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Oqtane.Client" Version="3.0.2" />
    <PackageReference Include="Oqtane.Shared" Version="3.0.2" />
  </ItemGroup>

  <PropertyGroup>
    <!-- there may be other elements here -->
    <BlazorWebAssemblyEnableLinking>false</BlazorWebAssemblyEnableLinking>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
